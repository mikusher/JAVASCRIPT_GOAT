				<HEAD>
					<SELECTOR />
					<OPTION VINDEX="0" SIZE="+100" SORT="0" DYNAM="1" SORTDIR="0" DEFAULTSORT="0" />
				</HEAD>
				<SOURCE>
					<FILE NAME="config/subdomains.dat" COMMENT="examples/subdomains.dat" DIALOG="subdomains" />
				</SOURCE>
				<BUTTONS>
					<BUTTON LABEL="TConfigForm_AddItemNext" TYPE="add" />
